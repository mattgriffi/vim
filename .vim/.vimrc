inoremap jk <ESC>
let mapleader=","
map <Enter> O<ESC>j
nnoremap <leader>s :set spell!<Enter>
filetype plugin indent on
syntax on
set number
set visualbell
set hlsearch
set encoding=utf-8
colorscheme Monokai
highlight Normal ctermbg=NONE
highlight nonText ctermbg=NONE
set spell spelllang=en_us
set nospell
